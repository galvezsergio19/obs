<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.5"
 xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 <display-name>SMG_OnlineBankingSystem</display-name>
 <servlet>
  <servlet-name>LoginServlet</servlet-name>
  <servlet-class>com.smg.controller.LoginServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>RegistrationServlet</servlet-name>
  <servlet-class>com.smg.controller.RegistrationServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>VerificationServlet</servlet-name>
  <servlet-class>com.smg.controller.VerificationServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>UserAthenticationServlet</servlet-name>
  <servlet-class>com.smg.controller.UserAthenticationServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>MypageServlet</servlet-name>
  <servlet-class>com.smg.controller.MypageServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>LogoutServlet</servlet-name>
  <servlet-class>com.smg.controller.LogoutServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>AtmServlet</servlet-name>
  <servlet-class>com.smg.controller.AtmServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>SmsServlet</servlet-name>
  <servlet-class>com.smg.controller.SmsServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>FundServlet</servlet-name>
  <servlet-class>com.smg.controller.FundServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>AccountServlet</servlet-name>
  <servlet-class>com.smg.controller.AccountServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>BalanceServlet</servlet-name>
  <servlet-class>com.smg.controller.BalanceServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>TransactionServlet</servlet-name>
  <servlet-class>com.smg.controller.TransactionServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>ProfileServlet</servlet-name>
  <servlet-class>com.smg.controller.ProfileServlet</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>LoginServlet</servlet-name>
  <url-pattern>/login</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>RegistrationServlet</servlet-name>
  <url-pattern>/register</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>VerificationServlet</servlet-name>
  <url-pattern>/verify</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>UserAthenticationServlet</servlet-name>
  <url-pattern>/user</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>MypageServlet</servlet-name>
  <url-pattern>/mypage</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>LogoutServlet</servlet-name>
  <url-pattern>/logout</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>AtmServlet</servlet-name>
  <url-pattern>/atm</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>SmsServlet</servlet-name>
  <url-pattern>/sms</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>FundServlet</servlet-name>
  <url-pattern>/fund-transfer</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>AccountServlet</servlet-name>
  <url-pattern>/account</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>BalanceServlet</servlet-name>
  <url-pattern>/balance</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>TransactionServlet</servlet-name>
  <url-pattern>/transaction</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ProfileServlet</servlet-name>
  <url-pattern>/profile</url-pattern>
 </servlet-mapping>
 <welcome-file-list>
  <welcome-file>index.html</welcome-file>
  <welcome-file>index.htm</welcome-file>
  <welcome-file>index.jsp</welcome-file>
  <welcome-file>default.html</welcome-file>
  <welcome-file>default.htm</welcome-file>
  <welcome-file>default.jsp</welcome-file>
 </welcome-file-list>
</web-app>
